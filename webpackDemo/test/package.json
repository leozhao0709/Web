{
  "name": "test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts-ts": "2.16.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css watch-css-module start-ts",
    "build": "npm-run-all build-css build-css-module build-ts",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build-css-module": "tcm src/ -o src/ --camelCase",
    "watch-css-module": "tcm src/ -o src/ --watch --camelCase --recursive",
    "start-ts": "react-scripts-ts start",
    "build-ts": "react-scripts-ts build"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.2",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "typed-css-modules": "^0.3.4",
    "typescript": "^2.9.1"
  }
}
